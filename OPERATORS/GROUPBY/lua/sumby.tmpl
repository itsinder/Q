return require 'Q/UTILS/lua/code_gen' {

   declaration = [[

#include "q_incs.h"
#include "_get_bit_u64.h"
${includes}
extern int
${fn}(  
      const ${val_ctype} * const restrict val_fld,  /* [nR_in] */
      uint64_t nR_in,
      const ${grpby_ctype} * const restrict grpby_fld,  /* [nR_in] */
      ${out_ctype} * restrict out_fld, /* [nR_out] */
      uint64_t nR_out,
      const uint64_t * const restrict cfld, 
      bool is_safe
      ) 
;

   ]],
   definition = [[

#include "_${fn}.h"

int
${fn}(  
      const ${val_ctype} * const restrict val_fld,  /* [nR_in] */
      uint64_t nR_in,
      const ${grpby_ctype} * const restrict grpby_fld,  /* [nR_in] */
      ${out_ctype} * restrict out_fld, /* [nR_out] */
      uint64_t nR_out,
      const uint64_t * const restrict cfld, 
      bool is_safe
      )

{ 
  int status = 0;

  // Note: we expect nR_out to be small. 
  for ( uint64_t i = 0; i < nR_out; i++ ) { 
    out_fld[i] = 0;
  }
  if ( is_safe ) {
#pragma omp parallel for schedule(static, 1024)
    for ( uint64_t i = 0; i < nR_in; i++ ) {
      ${grpby_ctype} grpby = grpby_fld[i];
      if ( ( grpby < 0 ) || ( grpby >= (${out_ctype})nR_out ) ) { 
        status = -1;
      }
    }
    cBYE(status);
  }
// TODO put back pragma
// #pragma omp parallel for schedule(static, 1024)
  if ( cfld != NULL ) { 
    for ( uint64_t i = 0; i < nR_in; i++ ) { 
      // TODO: Don't use a function call to extract the bits
      if ( get_bit_u64(cfld, i) == 1 ) { 
        ${val_ctype} val = val_fld[i];
        ${grpby_ctype} grpby = grpby_fld[i];
        out_fld[grpby] += val;
      }
    }
  }
  else {
    for ( uint64_t i = 0; i < nR_in; i++ ) { 
      ${val_ctype} val = val_fld[i];
      ${grpby_ctype} grpby = grpby_fld[i];
      out_fld[grpby] += val;
    }
  }
BYE:
  return status;
}
   ]]
}
