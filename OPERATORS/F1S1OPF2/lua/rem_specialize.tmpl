return function (
  in_qtype
  )
  local is_int_type = require 'Q/UTILS/lua/is_int_type'
  asssert(is_int_type(in_qtype), "in_qtype must be integer")
  local tmpl = 'arith.tmpl'
  local subs = {}
  subs.fn = "<<operator>>_" .. in_qtype 
  subs.in_ctype = assert(g_qtypes[in_qtype].ctype)
  subs.out_qtype = subs.in_ctype
  subs.out_ctype = assert(g_qtypes[out_qtype].ctype)
  subs.c_code_for_operator = " <<c_code>> "
  return subs, tmpl
end
